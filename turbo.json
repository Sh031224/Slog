{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "start": {
      "dependsOn": ["build"]
    },
    "test": {},
    "test:report": {}
  },
  "globalDependencies": [
    ".env",
    "tsconfig.json",
    "$NODE_ENV",
    "$HOST",
    "$USERNAME",
    "$PASSWORD",
    "$DATABASE",
    "$ADMIN_FACEBOOK_ID",
    "$JWT_SECRET",
    "$GOOGLE_PROJECT_ID",
    "$GOOGLE_PRIVATE_KEY_ID",
    "$GOOGLE_PRIVATE_KEY",
    "$GOOGLE_CLIENT_EMAIL",
    "$GOOGLE_CLIENT_ID",
    "$GOOGLE_CLIENT_X509_CERT_URL"
  ]
}
